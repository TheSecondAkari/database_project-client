{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.4.3",
    "cube-ui": "^1.12.34",
    "stylus-loader": "^3.0.2",
    "vant": "^2.2.16",
    "view-design": "^4.0.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.2"
  },
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/lib/${member}",
